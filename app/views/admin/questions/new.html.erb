<% provide(:title, 'Create a new question') %>
<h1>Create a new question</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for [:admin, @question] do |f| %>
      <%= render 'field', f: f %>
    <%= f.submit "Create question", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>